{
  "name": "frontend",
  "author": "MEBlabs",
  "description": "Fullstack test FrontEnd",
  "private": true,
  "scripts": {
    "start": "vite --port 3000",
    "build": "vite build --target=es2020",
    "preview": "vite preview",
    "default-theme": "node src/theme/ant.config.default.js && npx tailwindcss init --full src/theme/tailwind.config.default.js",
    "stylelint": "stylelint \"./**/*.css\"",
    "lint": "eslint . --ext .jsx,.js",
    "format": "prettier --write \"./**/*.{json,js,yml,md}\"",
    "format:check": "prettier --check \"./**/*.{json,js,yml,md}\""
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "antd": "^5.18.0",
    "antd-img-crop": "^4.22.0",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "i18n-iso-countries": "^7.11.2",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-flag-kit": "^1.1.1",
    "react-i18next": "^14.1.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-dom": "^6.23.1",
    "react-stickynode": "^4.1.1",
    "sprintf-js": "^1.1.3",
    "uuid": "^9.0.1"
